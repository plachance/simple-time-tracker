<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
	<entity name="SimpleTimeTracker\Entities\Task" table="task">
		<indexes>
			<index name="IDX_93872075C18272" columns="project_id"/>
			<index name="IDX_93872075FB88E14F" columns="user_id"/>
		</indexes>
		<id name="id" type="integer" column="task_id">
			<generator strategy="AUTO"/>
		</id>
		<field name="dateTimeBegin" type="datetime" column="date_time_begin" nullable="false"/>
		<field name="dateTimeEnd" type="datetime" column="date_time_end" nullable="true"/>
		<field name="description" type="text" column="description" nullable="true"/>
		<many-to-one field="user" target-entity="SimpleTimeTracker\Entities\User">
			<join-columns>
				<join-column name="user_id" referenced-column-name="user_id"/>
			</join-columns>
		</many-to-one>
		<many-to-one field="project" target-entity="SimpleTimeTracker\Entities\Project">
			<join-columns>
				<join-column name="project_id" referenced-column-name="project_id"/>
			</join-columns>
		</many-to-one>
	</entity>
</doctrine-mapping>
