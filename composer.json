{
	"name": "plachance/simple-time-tracker",
	"description": "Minimalist time tracking software.",
	"type": "project",
	"homepage": "https://github.com/plachance/simple-time-tracker",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/plachance/simple-time-tracker/issues"
	},
	"authors":
	[
		{
			"name": "Patrice Lachance",
			"email": "patricelachance@gmail.com",
			"homepage": "https://github.com/plachance",
			"role": "Developer"
		}
	],
	"repositories": [{
			"type": "package",
			"package":
			{
				"name": "istvan-ujjmeszaros/bootstrap-colorpickersliders",
				"type": "component",
				"version": "3.0.1",
				"dist":
				{
					"type": "zip",
					"url": "https://github.com/istvan-ujjmeszaros/bootstrap-colorpickersliders/archive/3.0.1.zip"
				},
				"require":
				{
					"robloach/component-installer": "*"
				},
				"extra":
				{
					"component":
					{
						"files":
						[
							"src/*.js",
							"src/*.css"
						]
					}
				}
			}
		},
		{
			"type": "package",
			"package":
			{
				"name": "bgrins/tinycolor",
				"type": "component",
				"version": "1.1.2",
				"dist":
				{
					"type": "zip",
					"url": "https://github.com/bgrins/TinyColor/archive/1.1.2.zip"
				},
				"require":
				{
					"robloach/component-installer": "*"
				},
				"extra":
				{
					"component":
					{
						"files":
						[
							"tinycolor.js"
						]
					}
				}
			}
		}],
	"require":
	{
		"php": ">=5.3",
		"ext-pdo": "*",
		"pradosoft/prado": "~3.2.0",
		"doctrine/orm": "~2.0",
		"doctrine/dbal": "2.4.2",
		"symfony/console": "~2.0",
		"components/jquery": "~2.0",
		"components/bootstrap": "~3.0",
		"swiftmailer/swiftmailer": "~5.0",
		"istvan-ujjmeszaros/bootstrap-colorpickersliders": "~3.0",
		"bgrins/tinycolor": "~1.0"
	},
	"autoload":
	{
		"psr-4":
		{
			"SimpleTimeTracker\\": "protected/"
		},
		"files": ["vendor/pradosoft/prado/framework/prado.php"]
	},
	"scripts":
	{
		"post-create-project-cmd":
		[
			"mkdir -p assets protected/runtime",
			"cp .htaccess.dist .htaccess",
			"cp protected/application.default.php protected/application.php",
			"php console.php orm:generate:proxies"
		],
		"post-install-cmd":
		[
			"mkdir -p assets protected/runtime",
			"php console.php orm:generate:proxies"
		],
		"post-update-cmd":
		[
			"mkdir -p assets protected/runtime",
			"php console.php orm:generate:proxies"
		]
	}
}